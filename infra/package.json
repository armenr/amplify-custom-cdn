{
  "bin": {
    "neos-infra": "bin/main.js"
  },
  "dependencies": {
    "@aws-cdk/aws-amplify": "^1.95.2",
    "@aws-cdk/aws-appsync": "1.95.2",
    "@aws-cdk/aws-certificatemanager": "1.95.2",
    "@aws-cdk/aws-cloudfront": "1.95.2",
    "@aws-cdk/aws-codecommit": "1.95.2",
    "@aws-cdk/aws-codepipeline": "1.95.2",
    "@aws-cdk/aws-codepipeline-actions": "1.95.2",
    "@aws-cdk/aws-dynamodb": "1.95.2",
    "@aws-cdk/aws-ec2": "1.95.2",
    "@aws-cdk/aws-iam": "1.95.2",
    "@aws-cdk/aws-kms": "^1.95.2",
    "@aws-cdk/aws-lambda": "1.95.2",
    "@aws-cdk/aws-rds": "1.95.2",
    "@aws-cdk/aws-route53": "1.95.2",
    "@aws-cdk/aws-route53-targets": "1.95.2",
    "@aws-cdk/aws-s3": "1.95.2",
    "@aws-cdk/aws-s3-deployment": "1.95.2",
    "@aws-cdk/cloud-assembly-schema": "1.95.2",
    "@aws-cdk/core": "1.95.2",
    "@aws-cdk/pipelines": "1.95.2",
    "aws-bootstrap-kit": "^0.3.9",
    "aws-sdk": "^2.890.0",
    "rocketcdk": "^0.2.1",
    "semver": "^7.3.5",
    "source-map-support": "^0.5.19"
  },
  "dependenciesComments": {
    "@aws-cdk/**": "ALL CDK LIBRARIES need to be KEPT AT THE SAME VERSION. Use `npx rocketdock` to update and pin all versions at once. SAME GOES FOR aws-bootstrap-kit! ",
    "aws-bootstrap-kit": "This provides a set of constructs that make life easier. The versions of @aws-cdk/* libs that are used in THIS construct library must be the same here!"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.95.2",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.41",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "aws-cdk": "1.95.2",
    "cdk-sso-sync": "0.0.6",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-upgrade": "^3.0.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "~4.2.4"
  },
  "name": "neos-infra",
  "scripts": {
    "build": "tsc",
    "build:all": "cd ../arubani/ && npm ci && npm run build && cd ../infra && npm ci && npm run build",
    "build:local": "npx tsc",
    "cdk": "cdk",
    "cdk:up": "npx rocketcdk up",
    "format": "prettier --check '**/*.ts' package.json .eslintrc.js .prettierrc tsconfig.json cdk.json cdk.context.json || exit 0",
    "format:fix": "prettier --write '**/*.ts' package.json .eslintrc.js .prettierrc tsconfig.json cdk.json cdk.context.json",
    "lint": "eslint -c .eslintrc.js --ext .ts, ./ --format table --cache",
    "lint:fix": "eslint -c .eslintrc.js --ext .ts, ./ --cache --fix --format table",
    "packages:up": "npx npm-upgrade",
    "switch:admin": "cdk-sso-sync admin && aws sso login --profile admin && cdk-sso-sync admin",
    "switch:cicd": "cdk-sso-sync cicd && aws sso login --profile cicd && cdk-sso-sync cicd",
    "switch:dev": "cdk-sso-sync dev && aws sso login --profile dev && cdk-sso-sync dev",
    "test": "jest",
    "watch": "npx tsc -w"
  },
  "version": "0.1.0"
}
